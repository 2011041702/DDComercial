<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chris.ddcomercial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Comercial">
        <activity android:name=".Activity.ActividadPrincipal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activity.ActividadDetalleCombo"
            android:parentActivityName=".Activity.ActividadPrincipal"
            android:label="Detalles de Combo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.ActividadDetalleCombo" />
        </activity>
    </application>

</manifest>